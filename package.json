    "express": "^4.17.1",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "supertest": "^6.1.3"
  },
  "devDependencies": {
    "sequelize-cli": "^6.2.0"
  }
}
