{
  "name": "phonebook",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run setup:db:test && npx jest --runInBand --detectOpenHandles",
    "test:watch": "npm run setup:db:test && npx jest --watch --runInBand --detectOpenHandles",
    "setup:db:test": "NODE_ENV=test npm run migrate:undo:all && NODE_ENV=test npm run migrate",
    "migrate:undo:all": "npx sequelize db:migrate:undo:all",
    "migrate": "npx sequelize db:migrate",
    "db:reset": "npm run migrate:undo:all && npm run migrate",
    "start": "nodemon ./src/index.js"
  },
  "jest": {
    "testEnvironment": "node"
  },
